<launch>
<include file="$(find realsense2_camera)/launch/rs_camera.launch">
    <arg name="camera" value="realsense_right"/>
    <arg name="serial_no" value="108222250646" />
    <arg name="initial_reset" value="true" />
    <arg name="json_file_path" value="$(find fewshot)/online_isec/launch/realsense_high_density.json"/>

    <arg name="clip_distance" value="1.0"/>
    <arg name="filters" value="pointcloud,temporal"/>
    <arg name="align_depth" value="true"/>

    <arg name="color_width" value="1280" />
    <arg name="color_height" value="720" />
    <arg name="color_fps" value="15" />

    <arg name="depth_width" value="848" />
    <arg name="depth_height" value="480" />
    <arg name="depth_fps" value="15" />
</include>

<rosparam>
    /camera/stereo_module/laser_power: 360
</rosparam>

</launch>